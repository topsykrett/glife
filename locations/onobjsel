	gs 'Menu.Create', 'disMenu'
	gs 'Menu.Add', 'disMenu', 'Delete an entry at the 1st row', '', 'MenuLoc', 'diszapis'
	gs 'Menu.Add', 'disMenu', 'Delete an entry at the 2nd row', '', 'MenuLoc', 'diszapis2'
